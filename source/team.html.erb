<div class="team text-center">
  <h1>Our team</h1>
  <% team = %w(anne seb) %>
  <ul class="list-inline text-center">
    <% team.each do |member| %>
      <li>
        <h2><%= member.capitalize %></h2>
        <%= image_tag "#{member}.jpg", class: "img-circle" %>
      <li>
    <% end %>
  </ul>

  <a href="#">contact@airbnb.com<i class="fa fa-envelope"></i></a>
</div>
